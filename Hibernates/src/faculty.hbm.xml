<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
    <hibernate-mapping>
    <class name="faculty" table="Faculty">
   
     <id name="fid" column="Faculty_id"></id>
   
     <property name="name" column="Faculty_name"></property>
     <property name="department" column="Faculty_department"></property>
     <property name="designation" column="Faculty_designation"></property>
     <property name="age" column="Faculty_age"></property>
     <property name="salary" column="Faculty_salary"></property>     
     

   
     </class>    
    
    </hibernate-mapping>